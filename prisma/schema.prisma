generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int     @id @default(autoincrement())
  uuid      String  @unique @default(uuid())
  email     String  @unique
  name      String?
  password  String
  matricula String  @unique
  tipo      String
}
